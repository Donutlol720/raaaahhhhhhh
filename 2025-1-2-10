import numpy as np
import matplotlib.pyplot as plt
import torch
import torch.nn as nn
import torch.optim as optim
torch.manual_seed(2025)
class My_ReLU(nn.Module):
  #no parameters
  def __init__(self):
    #subclasses 
    super().__init__()

  #callable
  def forward(self, x):
    #imitate relu
    return torch.max(torch.zeros_like(x), x)
